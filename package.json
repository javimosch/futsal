{
  "name": "futsal",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "Javier Leandro Arancibia <arancibiajav@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec \"npx cross-env NODE_ENV=development babel-node ./server --presets env\" --watch server --ext js,html,vue,css,scss",
    "build": "npx cross-env NODE_ENV=production webpack --config build/webpack.cordova.config.js --progress --hide-modules",
    "build:monaca":"yarn build && monaca remote build --browser",
    "build:watch": "npx cross-env WEBPACK_WATCH=true NODE_ENV=production webpack --config build/webpack.cordova.config.js --progress --hide-modules",
    "start": "npm run build-server && npm run start-server",
    "build-server": "npx cross-env NODE_ENV=production webpack --config build/webpack.node.config.js --progress --hide-modules",
    "start-server": "babel server -s -D -d dist --presets env && node dist"
  },
  "dependencies": {
    "auth0-js": "^9.2.0",
    "auth0-lock": "^11.2.1",
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
