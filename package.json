    "bootstrap-vue": "^2.0.0-rc.1",
    "clarify": "^2.1.0",
    "colors": "^1.1.2",
    "cors": "^2.8.4",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "express-jwt-authz": "^1.0.0",
    "faker": "^4.1.0",
    "file-loader": "^1.1.4",
    "gulp": "^3.9.1",
    "handlebars": "^4.0.11",
    "http-status": "^1.0.1",
    "is-directory": "^0.3.1",
    "jwks-rsa": "^1.2.1",
    "jwt-decode": "^2.2.0",
    "mongoose-multi": "^0.0.17",
    "morgan": "^1.9.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.14.12",
